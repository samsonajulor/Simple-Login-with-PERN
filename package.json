{
  "name": "postgres-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"tsc -w\" \"nodemon ./bin/www\"",
    "build": "tsc",
    "comp": "yarn build && yarn dev",
    "compile:watch": "yarn tsc -w",
    "start:prod": "NODE_ENV=production nodemon ./bin/www",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js --config src/pgconfig.ts"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.15",
    "pg": "^8.7.3",
    "postgres": "^1.0.2",
    "reflect-metadata": "^0.1.13",
    "tsc": "^2.0.4",
    "typeorm": "^0.2.41",
    "typescript": "^4.5.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.4",
    "ts-node": "^10.5.0"
  }
}
