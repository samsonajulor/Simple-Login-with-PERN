{
  "name": "postgres-project",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"tsc -w\" \"nodemon ./bin/www\"",
    "migrate": "ts-node node_modules/.bin/node-pg-migrate --no-reject-unauthorized -j ts -m ./src/migrations",
    "create-tables": "yarn migrate up",
    "delete-tables": "yarn migrate down",
    "build": "tsc",
    "test": "jest",
    "compile": "yarn tsc -w",
    "create-user": "yarn migrate create twitter",
    "clean": "rm -rf dist && mkdir dist",
    "heroku-postbuild": "yarn clean && yarn build"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/morgan": "^1.9.3",
    "@types/uuid": "^8.3.3",
    "bcrypt": "^5.0.1",
    "concurrently": "^7.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "http-errors": "^1.8.1",
    "jade": "~1.11.0",
    "jest": "^27.3.1",
    "joi": "^17.4.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "~1.9.1",
    "node-pg-migrate": "^6.0.0",
    "nodemailer": "^6.7.2",
    "nodemon": "^2.0.15",
    "pg": "^8.7.1",
    "postgres": "^2.0.0-beta.11",
    "ts-jest": "^27.0.7",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-flash": "^0.0.2",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.1",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/lodash": "^4.14.177",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/supertest": "^2.0.11",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
